<script type="text/javascript">
	function loadBlogPosts(){

	$.getJSON("/blog_posts.json", function(data){
		// console.log (data);

		var html = "";


		$.each(data, function (index){
		
			// alert(data[index].title);
			html += "<b>" + data[index].title + "</b><br>";

	});
			$("#blog_posts").html(html);

});


}


</script>

<div id="blog_posts" name="blog_posts">
	<a href="javascript:loadBlogPosts();">
	This is where blog posts go </a>
</div>